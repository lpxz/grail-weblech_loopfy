//<![CDATA[
 var current;

 $(function() {
  var navItems = $("#topNav > li > a");
  navItems.bind("mouseover", function() {
   $("#ads").hide();
   navItems.parent().removeClass("active");
   current = this;
   $(this).parent().addClass("active");
  });
 });
  
 function remove() {
  $(current).parent().removeClass("active");
 }
//]]>
</script>
